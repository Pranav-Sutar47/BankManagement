<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.finalbank.ClientMenu">
    <children>
        <VBox layoutX="5.0" layoutY="5.0" prefHeight="591.0" prefWidth="195.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <children>
                <ImageView fitHeight="150.0" fitWidth="180.0" pickOnBounds="true">
                    <VBox.margin>
                        <Insets bottom="5.0" left="15.0" right="5.0" top="5.0" />
                    </VBox.margin>
                    <image>
                        <Image url="@Images/bank.png" />
                    </image>
                </ImageView>
                <Label alignment="CENTER" prefHeight="17.0" prefWidth="183.0" text="ICICI BANK">
                    <VBox.margin>
                        <Insets left="10.0" right="5.0" top="10.0" />
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Label>
                <JFXButton fx:id="dashboard" buttonType="RAISED" onAction="#getDashboard" prefHeight="37.0" prefWidth="180.0" styleClass="btnMain" stylesheets="@Styles/ClientLogin.css" text="Dashboard">
                    <VBox.margin>
                        <Insets left="20.0" right="5.0" top="20.0" />
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                    <effect>
                        <DropShadow radius="7.8025" width="12.21">
                            <color>
                                <Color red="0.6509804129600525" green="0.6509804129600525" blue="0.6509804129600525" />
                            </color>
                        </DropShadow>
                    </effect>
                </JFXButton>
                <JFXButton fx:id="transaction" buttonType="RAISED" onAction="#getTransactions" prefHeight="37.0" prefWidth="180.0" styleClass="btnMain" stylesheets="@Styles/ClientLogin.css" text="Transactions">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="20.0" right="5.0" top="10.0" />
                    </VBox.margin>
                </JFXButton>
                <JFXButton fx:id="accounts" buttonType="RAISED" prefHeight="37.0" prefWidth="180.0" styleClass="btnMain" stylesheets="@Styles/ClientLogin.css" text="Accounts">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="10.0" left="20.0" right="5.0" top="10.0" />
                    </VBox.margin>
                </JFXButton>
                <Line endX="100.0" fill="#003300" startX="-100.0" strokeWidth="2.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="5.0" top="10.0" />
                    </VBox.margin>
                </Line>
                <JFXButton fx:id="profile" buttonType="RAISED" onAction="#getProfile" prefHeight="37.0" prefWidth="180.0" styleClass="btnColor" stylesheets="@Styles/ClientLogin.css" text="Profile">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="20.0" right="5.0" top="20.0" />
                    </VBox.margin>
                </JFXButton>
                <JFXButton fx:id="logout" buttonType="RAISED" onAction="#logOut" prefHeight="37.0" prefWidth="180.0" styleClass="btnColor" stylesheets="@Styles/ClientLogin.css" text="Logout">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="20.0" right="5.0" top="15.0" />
                    </VBox.margin>
                </JFXButton>
            </children>
        </VBox>
    </children>
</AnchorPane>
